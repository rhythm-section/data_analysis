<h1>Listing app_usages</h1>

<table>
  <thead>
    <tr>
      <th>App name</th>
      <th>Package name</th>
      <th>Version name</th>
      <th>Version code</th>
      <th>Base activity</th>
      <th>Top activity</th>
      <th>First install time</th>
      <th>Last update time</th>
      <th>App session</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_usages.each do |app_usage| %>
      <tr>
        <td><%= app_usage.app_name %></td>
        <td><%= app_usage.package_name %></td>
        <td><%= app_usage.version_name %></td>
        <td><%= app_usage.version_code %></td>
        <td><%= app_usage.base_activity %></td>
        <td><%= app_usage.top_activity %></td>
        <td><%= app_usage.first_install_time %></td>
        <td><%= app_usage.last_update_time %></td>
        <td><%= app_usage.app_session_id %></td>
        <td><%= link_to 'Show', app_usage %></td>
        <td><%= link_to 'Edit', edit_app_usage_path(app_usage) %></td>
        <td><%= link_to 'Destroy', app_usage, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App usage', new_app_usage_path %>
