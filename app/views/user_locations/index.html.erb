<h1>Listing user_locations</h1>

<table>
  <thead>
    <tr>
      <th>Location timestamp</th>
      <th>Location elapsed realtime nanos</th>
      <th>Location provider</th>
      <th>Location latitude</th>
      <th>Location longitude</th>
      <th>Location altitude</th>
      <th>Location speed</th>
      <th>Location accuracy</th>
      <th>Location app session</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_locations.each do |user_location| %>
      <tr>
        <td><%= user_location.location_timestamp %></td>
        <td><%= user_location.location_elapsed_realtime_nanos %></td>
        <td><%= user_location.location_provider %></td>
        <td><%= user_location.location_latitude %></td>
        <td><%= user_location.location_longitude %></td>
        <td><%= user_location.location_altitude %></td>
        <td><%= user_location.location_speed %></td>
        <td><%= user_location.location_accuracy %></td>
        <td><%= user_location.location_app_session_id %></td>
        <td><%= link_to 'Show', user_location %></td>
        <td><%= link_to 'Edit', edit_user_location_path(user_location) %></td>
        <td><%= link_to 'Destroy', user_location, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User location', new_user_location_path %>
