<h1>Listing app_sessions</h1>

<table>
  <thead>
    <tr>
      <th>App session timestamp start</th>
      <th>App session timestamp end</th>
      <th>App session timestamp duration</th>
      <th>Device</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @app_sessions.each do |app_session| %>
      <tr>
        <td><%= app_session.app_session_timestamp_start %></td>
        <td><%= app_session.app_session_timestamp_end %></td>
        <td><%= app_session.app_session_timestamp_duration %></td>
        <td><%= app_session.device_id %></td>
        <td><%= link_to 'Show', app_session %></td>
        <td><%= link_to 'Edit', edit_app_session_path(app_session) %></td>
        <td><%= link_to 'Destroy', app_session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New App session', new_app_session_path %>
