<h1>Listing devices</h1>

<table>
  <thead>
    <tr>
      <th>User name</th>
      <th>Mongo</th>
      <th>Uuid</th>
      <th>Android</th>
      <th>Created at</th>
      <th>Os version</th>
      <th>Model</th>
      <th>Product</th>
      <th>Manufacturer</th>
      <th>Brand</th>
      <th>User locations</th>
      <th>User activities</th>
      <th>App sessions</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @devices.each do |device| %>
      <tr>
        <td><%= device.user_name %></td>
        <td><%= device.mongo_id %></td>
        <td><%= device.uuid %></td>
        <td><%= device.android_id %></td>
        <td><%= device.created_at %></td>
        <td><%= device.os_version %></td>
        <td><%= device.model %></td>
        <td><%= device.product %></td>
        <td><%= device.manufacturer %></td>
        <td><%= device.brand %></td>
        <td><%= device.user_locations %></td>
        <td><%= device.user_activities %></td>
        <td><%= device.app_sessions %></td>
        <td><%= link_to 'Show', device %></td>
        <td><%= link_to 'Edit', edit_device_path(device) %></td>
        <td><%= link_to 'Destroy', device, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Device', new_device_path %>
